<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>				
		<!-- dev sqlserver database  -->
		<property name="connection.datasource">wealth/ds/StaticData_DS</property>		
		<property name="dialect">org.hibernate.dialect.SQLServerDialect</property>
        <property name="show_sql">true</property>   	
			
		<!-- Mappings -->
		<mapping class="com.wealth.staticdata.domain.AccountType" />
		<mapping class="com.wealth.staticdata.domain.ContactType" />
		<mapping class="com.wealth.staticdata.domain.PropertyType" />
		<mapping class="com.wealth.staticdata.domain.BranchType" />
		<mapping class="com.wealth.staticdata.domain.BranchTypePrivateClient" />
		<mapping class="com.wealth.staticdata.domain.CardType" />
		<mapping class="com.wealth.staticdata.domain.CardFIID" />
		<mapping class="com.wealth.staticdata.domain.ProductHouse" />
		
	</session-factory>
</hibernate-configuration>